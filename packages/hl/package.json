{
  "name": "@transmute/hl",
  "author": "Orie Steele",
  "module": "dist/hl.esm.js",
  "version": "0.0.4-unstable.2",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "TS_JEST_DISABLE_VER_CHECKER=true tsdx test",
    "lint": "tsdx lint src --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/hl.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hl.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.2",
    "husky": "^4.3.8",
    "jsonld": "^3.3.0",
    "size-limit": "^4.9.2",
    "ts-jest": "^26.5.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "blakejs": "^1.1.0",
    "borc": "^2.1.2",
    "bs58": "^4.0.1",
    "isomorphic-webcrypto": "^2.3.6"
  },
  "gitHead": "f7517b0fe6e8ab46eab7afc36e368f35880d2daf",
  "homepage": "https://github.com/transmute-industries/linked-data/tree/main/packages/hl",
  "repository": {
    "type": "git",
